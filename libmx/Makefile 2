CFLGS = -std=c11 -Wall -Wextra -Wpedantic -Werror 
all: install clean

install:
	@cp $(INC) $(SRCS) .
	@clang $(CFLGS) -c  $(SRC)
	@mkdir  obj
	@mv $(OBJ) ./obj
	@ar -rc $(NAME) $(OBJO)

uninstall:
	@rm -rf $(NAME)
	@rm -rf $(OBJO) $(SRC) $(HEADTMP)
	@rm -rf ./obj

clean:
	@rm -rf $(OBJO) $(SRC) $(HEADTMP)
	@rm -rf ./obj
reinstall: clean all
